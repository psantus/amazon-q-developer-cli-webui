# Copy this file to terraform.tfvars and fill in your values

aws_region = "us-east-1"
project_name = "q-cli-webui"
cognito_username = "qcli-user"
cognito_user_email = "your-email@example.com"

# Optional: Custom password (leave empty for auto-generated)
# cognito_user_password = "YourSecurePassword123!"

# Optional: Custom domain with Route53 (requires hosted zone)
# domain_name = "example.com"        # Root domain (must have Route53 hosted zone)
# subdomain = "qcli"                 # Creates qcli.example.com (certificate auto-created)
