  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "babel-loader": "^10.0.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^7.1.2",
    "fs-extra": "^11.3.1",
    "html-webpack-plugin": "^5.5.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "style-loader": "^4.0.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "webpack": "^5.88.0",
